{
  "name": "phonograph",
  "version": "1.3.3",
  "description": "",
  "homepage": "/",
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^6.1.10",
    "@mui/material": "^6.1.10",
    "audioqueue": "^1.1.1",
    "colorthief": "^2.4.0",
    "dayjs": "^1.8.23",
    "dompurify": "^2.0.10",
    "encoding": "^0.1.12",
    "idb-keyval": "^3.2.0",
    "node-fetch": "^2.6.0",
    "podcastsuite": "1.1.9",
    "randomcolor": "^0.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "5.1.2",
    "react-router-dom": "^5.1.2",
    "smallfetch": "^1.0.8",
    "xml2js": "^0.4.22",
    "zustand": "^5.0.3"
  },
  "private": true,
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "scripts": {
    "start": "vite",
    "build": "yarn getter && vite build && yarn SEO && yarn sw && yarn lambda:build",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "SEO": "cp ./public/robots.txt ./dist/robots.txt",
    "bundle": "vite build",
    "lambda:build": "netlify functions:build --src ./lambda --functions .netlify/functions-dist",
    "lambda:serve": "netlify functions:serve --src ./lambda",
    "dev": "yarn getter && concurrently \"yarn lambda:serve\" \"vite\"",
    "getter": "bash ./filegetter.sh",
    "sw": "node swGenerator.js"
  },
  "alias": {
    "public": "/public/"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@netlify/functions": "^5.1.2",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^25.2.3",
    "@types/node-fetch": "2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "5",
    "@vitejs/plugin-react": "^5.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "concurrently": "^9.2.1",
    "cz-conventional-changelog": "3.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.3",
    "netlify-cli": "^23.4.3",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
